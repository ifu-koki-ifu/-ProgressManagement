        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Add your code here

    //新規で、回線手配担当者有りなら、アサイン済み
    if (current.u_progress == '0' && !current.u_kaisen_tehai.nil()) {
        current.u_progress.setValue('5');
        //アサイン済みで、(何かしら更新が入ったら)、手配中
    } else if (current.u_progress == '5') {
        current.u_progress.setValue('1');
        // 手配中で、オーダ完了日に入力有りなら、オーダ完了
    } else if (current.u_progress == '1' && !current.u_order_kanryo_day.nil()) {
        current.u_progress.setValue('2');
        // オーダ完了で、作業完了日に入力有りなら、作業完了
    } else if (current.u_progress == '2' && !current.u_sagyo_kanryo_day.nil()) {
        current.u_progress.setValue('4');
    }

})(current, previous);]]></script>
